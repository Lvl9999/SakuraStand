if #game:GetService("Players"):GetPlayers() <= 3 then
    FarmingTab.newLabel("‚ùó Auto Item Farmer + Auto Boss/Mob Farmer");
    FarmingTab.newToggle("Start Farming [Private Server Edition]","",false,function(Value)
        getgenv().AutoPrivateGrinding = Value
        getgenv().ListOfNpcs = {"Space Curse","Jotaro Kujo","Toji","Dio","Deku","Adjuchas","Contorted Curse","Glutton Curse","Mosquito Curse","Spider Curse","Frog Hollow","Fishbone"};
        
        if game:GetService("Players").LocalPlayer.Data.StandName.Value == "Gojo Satoru" or game:GetService("Players").LocalPlayer.Data.StandName.Value == "Standless" then
            getgenv().Distance = 5;
        elseif game:GetService("Players").LocalPlayer.Data.StandName.Value == "Shadow The World" then
            getgenv().Distance = 7;
        end

        if game:GetService("Players").LocalPlayer.Data.Achievements["Boss Raid"] and game:GetService("Players").LocalPlayer.Data.Achievements["The Intelligent"] and game:GetService("Players").LocalPlayer.Data.StandName.Value == "Standless" or game:GetService("Players").LocalPlayer.Data.StandName.Value == "Shadow The World" or game:GetService("Players").LocalPlayer.Data.StandName.Value == "Gojo Satoru" then
            task.spawn(function()
                while getgenv().AutoPrivateGrinding == true do
                    pcall(function()
                        local Target = nil
                        getgenv().FoundAnything = false
                     
                        for i,v in ipairs(getgenv().ListOfNpcs) do
                            if workspace.Living:FindFirstChild(v) and workspace.Living:FindFirstChild(v):FindFirstChild("Head") then
                                local NPC = workspace.Living:FindFirstChild(v);
                                if NPC then
                                    Target = NPC
                                    getgenv().FoundAnything = true
                                    break
                                end
                            end
                        end
                     
                        if Target and game.Players.LocalPlayer.Character.Humanoid.Health ~= 0 then
                            if Target:FindFirstChild("Participated") and Target.Participated:FindFirstChild(game.Players.LocalPlayer.Name) and Target.Participated[game.Players.LocalPlayer.Name].Value > 40 then
                                Target.Humanoid.Health = 0
                            end

                            if game:GetService("Players").LocalPlayer.Data.Achievements["Boss Raid"] and game:GetService("Players").LocalPlayer.Data.AppliedACM.Value == "Boss Raid" then
                                game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(Target.HumanoidRootPart.Position - Target.HumanoidRootPart.CFrame.lookVector * getgenv().Distance,Target.HumanoidRootPart.Position);getgenv().Attacking();
                            end
                        else
                            task.wait(2.5);
                        end
                    end)                     
                    task.wait(0.015);
                end
            end)
        
            task.spawn(function()
                while getgenv().AutoPrivateGrinding == true do
                    pcall(function()
                        if getgenv().FoundAnything == true and not game:GetService("Players").LocalPlayer.PlayerGui.InCombat.Enabled then
                            if game:GetService("Players").LocalPlayer.Data.Achievements["Boss Raid"] and game:GetService("Players").LocalPlayer.Data.AppliedACM.Value ~= "Boss Raid" then
                                local args = {[1] = "Boss Raid"};
                                game:GetService("ReplicatedStorage"):WaitForChild("GlobalUsedRemotes"):WaitForChild("ApplyAchievement"):FireServer(unpack(args));
                            end
                        elseif getgenv().FoundAnything == false and not game:GetService("Players").LocalPlayer.PlayerGui.InCombat.Enabled then
                            if game:GetService("Players").LocalPlayer.Data.Achievements["The Intelligent"] and game:GetService("Players").LocalPlayer.Data.AppliedACM.Value ~= "The Intelligent" then
                                local args = {[1] = "The Intelligent"};
                                game:GetService("ReplicatedStorage"):WaitForChild("GlobalUsedRemotes"):WaitForChild("ApplyAchievement"):FireServer(unpack(args));
                            end
                        elseif getgenv().FoundAnything == false and game:GetService("Players").LocalPlayer.PlayerGui.InCombat.Enabled then
                            game.Players.LocalPlayer.Character.Humanoid.Health = 0
                        end            
                    end)
                    task.wait(0.35);
                end
            end)
        
            task.spawn(function()
                while getgenv().AutoPrivateGrinding == true do
                        pcall(function()
                            if getgenv().FoundAnything == false and not game:GetService("Players").LocalPlayer.PlayerGui.InCombat.Enabled then
                                if game:GetService("Players").LocalPlayer.Data.Achievements["The Intelligent"] and game:GetService("Players").LocalPlayer.Data.AppliedACM.Value == "The Intelligent" then
                                    local RespawnNeeded = false
        
                                    for _,k in pairs(game.Workspace.Item:GetChildren()) do
                                        if ((k.Name == "Box" and k:FindFirstChild("Outline")) and k.Position ~= Vector3.new(1804.5390625,-298.173583984375,-309.4649658203125)) or k.Name == "Barrel" or k.Name == "Chest" and game:GetService("Players").LocalPlayer.Backpack:FindFirstChild("Chest Key") then
                                            RespawnNeeded = true
                                            break
                                        end
                                    end
        
                                    if RespawnNeeded and getgenv().AutoLevelingUp == false then
                                        local args = {[1] = "Exploiter"};
                                        game:GetService("ReplicatedStorage"):WaitForChild("GlobalUsedRemotes"):WaitForChild("ApplyAchievement"):FireServer(unpack(args));task.wait(4.5);
                                    end
                                end
                            end
                        end)
                    task.wait(0.35);
                end
            end)
    
            task.spawn(function()
                while getgenv().AutoPrivateGrinding == true do
                    pcall(function()
                        if getgenv().FoundAnything == false and not game:GetService("Players").LocalPlayer.PlayerGui.InCombat.Enabled then
                            local Lplayer = game:GetService("Players").LocalPlayer

                            local A1 = game:GetService("Workspace"):FindFirstChild("Item");
                            local A2 = game:GetService("Workspace"):FindFirstChild("Item2");
                            
                            local Parts = {}
                            
                            local function Checks(Item)
                                if Item then
                                    for _,k in ipairs(Item:GetChildren()) do
                                        if k:IsA("BasePart") and k.Name ~= "SoulSpawn" and k.Name ~= "PerceptionMask" and k.Position ~= Vector3.new(1804.5390625,-298.173583984375,-309.4649658203125) then
                                            if (((k.Name:sub(1,2) == "Bo" or k.Name:sub(1,3) == "Sec") and k:FindFirstChild("Outline"))) or (((k.Name:sub(1,2) ~= "Bo" and k.Name:sub(1,3) ~= "Sec") and not k:FindFirstChild("Outline"))) then
                                                if not ((Lplayer.Data.Mastery.Value == 0) and k.Name == "SukunaFinger") and not ((Lplayer.Data.Mastery.Value == 0) and k.Name == "Hogyoku Fragment") then
                                                    if (Lplayer.Backpack:FindFirstChild("Chest Key") and game.Workspace.Item:FindFirstChild("Chest") and k.Name == "Chest") or (not Lplayer.Backpack:FindFirstChild("Chest Key") and game.Workspace.Item:FindFirstChild("Chest") and k.Name ~= "Chest") then
                                                        table.insert(Parts,k);
                                                    elseif not game.Workspace.Item:FindFirstChild("Chest") then
                                                        table.insert(Parts,k);
                                                    end
                                                end
                                            end
                                        end
                                    end
                                end
                            end
                            
                            Checks(A1);Checks(A2);
                            
                            if #Parts > 0 then
                                local Selected = Parts[math.ceil((1 - math.sqrt(math.random())) * #Parts)];

                                if Selected then
                                    Lplayer.Character:SetPrimaryPartCFrame(CFrame.new(Selected.Position - Vector3.new(0,-2.5,0)));task.wait(0.15);
                                    for _,Items in ipairs({A1,A2}) do
                                        for _,v in pairs(Items:GetDescendants()) do
                                            if v:IsA("ProximityPrompt") and v.Parent.Name ~= "SoulSpawn" then
                                                fireproximityprompt(v,0);
                                            end
                                        end
                                    end
                                end
                            end
                        end
                    end)
                    task.wait(0.15);
                end
            end)
    
            task.spawn(function()
                while getgenv().AutoPrivateGrinding == true do
                    pcall(function()
                        local args = {[1] = "/e sadcat"};
                        game:GetService("ReplicatedStorage"):WaitForChild("GlobalUsedRemotes"):WaitForChild("Chatted"):FireServer(unpack(args));
                    end)
                    task.wait(0.35);
                end
            end)
    
            task.spawn(function()
                if getgenv().AutoPrivateGrinding == true then
                    workspace.Gravity = 9e9;
                elseif getgenv().AutoPrivateGrinding == false then
                    workspace.Gravity = 196.2;
                end
            end)
        else
            if getgenv().AutoPrivateGrinding == true then
                BoredLibrary.prompt("Emulator Hub   üíª", "You don't have the required stuff...");
            end
        end
    end)
else
    FarmingTab.newLabel("‚ùó Auto Item Farmer + Auto Boss/Mob Farmer");
    FarmingTab.newToggle("Start Farming [Public Server Edition]","",false,function(Value)
        getgenv().AutoPublicGrinding = Value
        getgenv().ListOfNpcs = {"Space Curse","Jotaro Kujo","Toji","Dio","Deku","Adjuchas","Contorted Curse","Glutton Curse","Mosquito Curse","Spider Curse","Frog Hollow","Fishbone"};
        getgenv().FoundAdjuchas = false;

        local FloatPart = Instance.new("Part",game.Workspace);
        
        FloatPart.Name = "FloatPart"
        FloatPart.Size = Vector3.new(6,0.1,6);
        FloatPart.Anchored = true
        FloatPart.Transparency = 1

        task.spawn(function()
            while getgenv().AutoPublicGrinding == true do
                pcall(function()
                    FloatPart.CFrame = game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame + Vector3.new(0,-3.05,0);
                end)
                task.wait(0.001);
            end
        end)

        task.spawn(function()
            if getgenv().AutoPublicGrinding == false then
                game.Players.LocalPlayer.Character.Humanoid.Health = 0
            end
        end)

        task.spawn(function()
            while getgenv().AutoPublicGrinding == true do
                pcall(function()
                    for _,j in pairs(game:GetService("Players"):GetPlayers()) do
                        local Char = j.Character
                        if Char then
                            for _,n in pairs(Char:GetDescendants()) do
                                if n:IsA("BasePart") then
                                    n.CanCollide = false
                                end
                            end
                        end
                    end
                end)
                task.wait(0.15);
            end
        end)

        task.spawn(function()
            if game:GetService("Players").LocalPlayer.Data.StandName.Value == "The Black Silence" then
                while getgenv().AutoPublicGrinding == true do
                    pcall(function()
                        local Target = nil

                        for i,v in ipairs(getgenv().ListOfNpcs) do
                            if workspace.Living:FindFirstChild(v) and workspace.Living:FindFirstChild(v):FindFirstChild("Head") then
                                local NPC = workspace.Living:FindFirstChild(v);
                                if NPC then
                                    Target = NPC
                                    break
                                end
                            end
                        end

                        if Target and game.Players.LocalPlayer.Character.Humanoid.Health ~= 0 then
                            if Target:FindFirstChild("Participated") and Target.Participated:FindFirstChild(game.Players.LocalPlayer.Name) then
                                task.spawn(function()task.wait(1.5);
                                    Target.Humanoid.Health = 0
                                end)
                            end

                            if Target.Name == "Adjuchas" then
                                getgenv().FoundAdjuchas = true;
                                game.Players.LocalPlayer.Character:SetPrimaryPartCFrame(CFrame.new(Target.PrimaryPart.Position - Vector3.new(0,20,0)));
                            else
                                getgenv().FoundAdjuchas = false;
                            end

                            local args = {[1] = Target};
                            game:GetService("ReplicatedStorage"):WaitForChild("RolandRemote"):WaitForChild("Mook"):FireServer(unpack(args));
                        else
                            task.wait(2.5);
                        end
                    end)                    
                    task.wait(0.15);
                end
            else
                if getgenv().AutoPublicGrinding == true then
                    BoredLibrary.prompt("Warning   ‚ö†Ô∏è"," Using Roland enables risky autofarm",5);
                end
            end
        end)

        task.spawn(function()
            while getgenv().AutoPublicGrinding == true do
                pcall(function()
                    if getgenv().FoundAdjuchas == false then
                        local Lplayer = game:GetService("Players").LocalPlayer
                        local A1 = game:GetService("Workspace"):FindFirstChild("Item");
                        local A2 = game:GetService("Workspace"):FindFirstChild("Item2");
            
                        local Parts = {}
            
                        local function Checks(Item)
                            if Item then
                                for _,k in ipairs(Item:GetChildren()) do
                                    if k:IsA("BasePart") and k.Name ~= "SoulSpawn" and k.Name ~= "PerceptionMask" and k.Position ~= Vector3.new(1804.5390625,-298.173583984375,-309.4649658203125) then
                                        if (((k.Name:sub(1,2) == "Bo" or k.Name:sub(1,3) == "Sec") and k:FindFirstChild("Outline"))) or (((k.Name:sub(1,2) ~= "Bo" and k.Name:sub(1,3) ~= "Sec") and not k:FindFirstChild("Outline"))) then
                                            if not ((Lplayer.Data.Mastery.Value == 0) and k.Name == "SukunaFinger") and not ((Lplayer.Data.Mastery.Value == 0) and k.Name == "Hogyoku Fragment") then
                                                if (Lplayer.Backpack:FindFirstChild("Chest Key") and game.Workspace.Item:FindFirstChild("Chest") and k.Name == "Chest") or (not Lplayer.Backpack:FindFirstChild("Chest Key") and game.Workspace.Item:FindFirstChild("Chest") and k.Name ~= "Chest") then
                                                    table.insert(Parts,k);
                                                elseif not game.Workspace.Item:FindFirstChild("Chest") then
                                                    table.insert(Parts,k);
                                                end
                                            end
                                        end
                                    end
                                end
                            end
                        end                 
    
                        Checks(A1);Checks(A2);
            
                        if #Parts > 0 then
                            local Selected = Parts[math.ceil((1 - math.sqrt(math.random())) * #Parts)];
            
                            if Selected then
                                Lplayer.Character:SetPrimaryPartCFrame(CFrame.new(Selected.Position - Vector3.new(0,6,0)));task.wait(0.35);
                                for _,Items in ipairs({A1,A2}) do
                                    for _,v in pairs(Items:GetDescendants()) do
                                        if v:IsA("ProximityPrompt") and v.Parent.Name ~= "SoulSpawn" then
                                            fireproximityprompt(v,0);
                                        end
                                    end
                                end
                            end
                        end
                    end
                end)
                task.wait(0.35);
            end
        end)

        task.spawn(function()
            while getgenv().AutoPublicGrinding == true do
                pcall(function()
                    local args = {[1] = "/e sadcat"};
                    game:GetService("ReplicatedStorage"):WaitForChild("GlobalUsedRemotes"):WaitForChild("Chatted"):FireServer(unpack(args));
                end)
                task.wait(0.35);
            end
        end)
    end)
end

