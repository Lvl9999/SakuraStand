--[[---------------------------------------------------------------------------------------------------
    This script updates a lot, Use this loadstring to be in the latest version:

    getgenv().ToggleUI = "J"-- //  Key Toggle "J" set by default however you can change it.
    getgenv().SakuraHub = "https://raw.githubusercontent.com/Lvl9999/SakuraStand/main/SakuraHub";

    pcall(function()
        loadstring(game:HttpGet(getgenv().SakuraHub))();
    end)
]]-----------------------------------------------------------------------------------------------------

-- // Auto Loader \\ --
repeat task.wait() until game:IsLoaded();

-- // Anti AFK \\ --
task.spawn(function()
    if not getgenv().AntiAfk == true then
        while true do task.wait(250);
            pcall(function()
                game:GetService("VirtualInputManager"):SendKeyEvent(true,"RightBracket",false,game);
            end)
        end
        getgenv().AntiAfk = true
    end
end)

-- // Auto Skip Loading Menu \\ --
task.spawn(function()
    pcall(function()
        for i = 1,25 do
            local PlayButton = game:GetService("Players").LocalPlayer.PlayerGui.MainMenu.Buttons.Play.TextLabel
            local SkipButton = game:GetService("Players").LocalPlayer.PlayerGui.Loading.BG.SkipButton

            game:GetService("VirtualInputManager"):SendMouseButtonEvent(PlayButton.AbsolutePosition.X+PlayButton.AbsoluteSize.X/5,PlayButton.AbsolutePosition.Y+50,0,true,PlayButton,5);
            game:GetService("VirtualInputManager"):SendMouseButtonEvent(PlayButton.AbsolutePosition.X+PlayButton.AbsoluteSize.X/5,PlayButton.AbsolutePosition.Y+50,0,false,PlayButton,5);

            game:GetService("VirtualInputManager"):SendMouseButtonEvent(SkipButton.AbsolutePosition.X+SkipButton.AbsoluteSize.X/5,SkipButton.AbsolutePosition.Y+50,0,true,SkipButton,5);
            game:GetService("VirtualInputManager"):SendMouseButtonEvent(SkipButton.AbsolutePosition.X+SkipButton.AbsoluteSize.X/5,SkipButton.AbsolutePosition.Y+50,0,false,SkipButton,5);
            task.wait(1);
        end
    end)
end)

-- // Reset Configs \\ --
getgenv().ResetConfigs = function()
    getgenv().AutoPrivateGrinding = false;
    getgenv().AutoPublicGrinding = false;
    getgenv().AutoBreakthrough = false;
    getgenv().AutoLevelingUp = false;
    getgenv().AutoRebirth = false;
    getgenv().AutoSelling = false;

    getgenv().AutoParryingDamage = false;
    getgenv().AutoBlockingDamage = false;
    getgenv().AutoDealingDamage = false;
    getgenv().AutoTakingDamage = false;
    getgenv().AutoDyingDamage = false;

    getgenv().StoringEveryItem = false;
    getgenv().StoringNikyu = false;
    getgenv().StoringFinger = false;
    getgenv().StoringInverted = false;
    getgenv().StoringSplit = false;
    getgenv().StoringChain = false;
    getgenv().StoringCortex = false;
    getgenv().StoringCrimson = false;
    getgenv().StoringCyan = false;
    getgenv().StoringTech = false;
    getgenv().StoringArasaka = false;
    getgenv().StoringKujoHat = false;
    getgenv().StoringDiosBone = false;
    getgenv().StoringDiosDiary = false;
    getgenv().StoringHogyoku = false;

    getgenv().WalkspeedBypass = false;

    getgenv().AutoGlitching = false;
    getgenv().AutoEscaping = false;
    getgenv().AutoDodging = false;

    workspace.Gravity = 196.2;
end

getgenv().ResetConfigs();
-----------------------------------[[ Main Ui Libs ]]-----------------------------------------------------------------
local DrRayLibrary = loadstring(game:HttpGet("https://raw.githubusercontent.com/Lvl9999/DrRay/main/Backup"))();
local BoredLibrary = loadstring(game:HttpGet("https://raw.githubusercontent.com/Lvl9999/Bored/main/Backup"))();
----------------------------------------------------------------------------------------------------------------------

---------------------------------[[ Main Source Code ]]---------------------------------------------------------------
local MainWindow = DrRayLibrary:Load("Sakura Hub   üå∏","Default");
BoredLibrary.prompt("Sakura Hub   üå∏","Successfully Loaded :3",2);

local HomeTab = DrRayLibrary.newTab("Home","ImageIdHere");

HomeTab.newLabel("Local Player Scripts   ‚ú®");
HomeTab.newToggle("Auto Run Faster","",false,function(Value)
    getgenv().WalkspeedBypass = Value

    -- Got From https://raw.githubusercontent.com/EdgeIY/infiniteyield/master/source
    task.spawn(function()
        while getgenv().WalkspeedBypass == true and game:GetService("Players").LocalPlayer do
            pcall(function()
                local chr = game:GetService("Players").LocalPlayer.Character
                if chr then
                   local hum = chr:FindFirstChildWhichIsA("Humanoid");
                    if hum and hum.Parent then
                        local hb = game:GetService("RunService").Heartbeat
                        local delta = hb:Wait()
                        if hum.MoveDirection.Magnitude > 0 then
                            chr:TranslateBy(hum.MoveDirection * 10 * delta * 10);
                        else
                            chr:TranslateBy(hum.MoveDirection * delta * 10);
                        end
                    end
                end
            end)
            task.wait(0.0015);
        end
    end)
end)

HomeTab.newLabel("Fighting Scripts   ‚öîÔ∏è");
HomeTab.newToggle("Auto Escape Combos (Use In Open Areas)","",false,function(Value)
    getgenv().AutoEscaping = Value

    task.spawn(function()
        while getgenv().AutoEscaping == true do
            pcall(function()
                if workspace.Living[game.Players.LocalPlayer.Name].CDValues:FindFirstChild("Stun") and not workspace.Living[game.Players.LocalPlayer.Name].CDValues:FindFirstChild("OnCooldown") then
                    game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame * CFrame.new(0,0,1.35);
                end
            end)
            task.wait(0.00015);
        end
    end)
end)

HomeTab.newToggle("Auto Dodge (Use In Open Areas)","",false,function(Value)
    getgenv().AutoDodging = Value

    task.spawn(function()
        while getgenv().AutoDodging == true do
            pcall(function()
                for _,k in ipairs(workspace.Living:GetChildren()) do
                    if k:IsA("Model") and k:FindFirstChild("Head") and k.Head:IsA("Part") and k.Head.Name == "Head" and k.Head ~= game.Players.LocalPlayer.Character.Head then
                        if (k.Head.Position - game.Players.LocalPlayer.Character.Head.Position).magnitude <= 25 then
                            if k.CDValues:FindFirstChild("OnCooldown") and not workspace.Living[game.Players.LocalPlayer.Name].CDValues:FindFirstChild("OnCooldown") and not k.CDValues:FindFirstChild("Posing") then
                                game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(k.Head.Position + Vector3.new(math.random(-30,30),0, math.random(-30,30)));
                                for _,k in ipairs(workspace.Effects:GetChildren()) do
                                    if k.Name == "HitboxDisplayPart" and k:IsA("BasePart") then
                                        if (k.Position - game.Players.LocalPlayer.Character.HumanoidRootPart.Position).magnitude <= 65 then
                                            game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(k.Position + Vector3.new(math.random(-35,25),0,math.random(-31,41)));
                                        end
                                    end
                                end
                            end
                        end
                    end
                end
            end)
            task.wait(0.0000000000000000000000000000015);
        end
    end)
end)

HomeTab.newToggle("Auto Bug Players / Mobs (Requires Grab Moves)","",false,function(Value)
    getgenv().AutoGlitching = Value

    task.spawn(function()
        local X = game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame
        local Teleported = false;

        while getgenv().AutoGlitching == true do
            pcall(function()
                local OnCooldown = workspace.Living[game.Players.LocalPlayer.Name].CDValues:FindFirstChild("OnCooldown");
                local Dashing = workspace.Living[game.Players.LocalPlayer.Name].CDValues:FindFirstChild("Dashing");
                
                if OnCooldown and Dashing and not Teleported then
                    X = game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame;
                    game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(-9000,-150,9000);
                    Teleported = true;
                elseif not OnCooldown and not Dashing and Teleported then
                    game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = X;
                    Teleported = false;
                end
            end)
            task.wait(0.35);
        end
    end)
end)

