    FarmingTab.newToggle("Start Farming  [Private Server Edition]","",false,function(Value)
        getgenv().AutoPrivateGrinding = Value
        getgenv().ListOfNpcs = {"Space Curse","Jotaro Kujo","Toji","AngelicaWeak","Roland","Dio","Deku","Adjuchas","Contorted Curse","Glutton Curse","Mosquito Curse","Spider Curse","Frog Hollow","Fishbone"};

        if game:GetService("Players").LocalPlayer.Data.StandName.Value == "Standless" or game:GetService("Players").LocalPlayer.Data.StandName.Value == "Shinra Kusakabe" then
            getgenv().Distance = 5;
        elseif game:GetService("Players").LocalPlayer.Data.StandName.Value == "Shadow The World" then
            getgenv().Distance = 7;
        end

        if game:GetService("Players").LocalPlayer.Data.Achievements["The Intelligent"] and game:GetService("Players").LocalPlayer.Data.StandName.Value == "Standless" or game:GetService("Players").LocalPlayer.Data.StandName.Value == "Shadow The World" or game:GetService("Players").LocalPlayer.Data.StandName.Value == "Shinra Kusakabe" then
            task.spawn(function()
                while getgenv().AutoPrivateGrinding == true do
                    pcall(function()
                        local Target = nil
                        getgenv().FoundAnything = false

                        for i,v in ipairs(getgenv().ListOfNpcs) do
                            if workspace.Living:FindFirstChild(v) and workspace.Living:FindFirstChild(v):FindFirstChild("Head") then
                                local NPC = workspace.Living:FindFirstChild(v);
                                if NPC then
                                    Target = NPC
                                    getgenv().FoundAnything = true
                                    break
                                end
                            end
                        end

                        if Target and game.Players.LocalPlayer.Character.Humanoid.Health ~= 0 then
                            if Target:FindFirstChild("Participated") and Target.Participated:FindFirstChild(game.Players.LocalPlayer.Name) then
                                Target.Humanoid.Health = 0
                            end

                            if game:GetService("Players").LocalPlayer.Data.Achievements["The Intelligent"] and game:GetService("Players").LocalPlayer.Data.AppliedACM.Value == "The Intelligent" then
                                game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(Target.HumanoidRootPart.Position - Target.HumanoidRootPart.CFrame.lookVector * getgenv().Distance,Target.HumanoidRootPart.Position);getgenv().Attacking();
                            end
                        else
                            task.wait(2.5);
                        end
                    end)
                    task.wait(0.015);
                end
            end)
        
            task.spawn(function()
                while getgenv().AutoPrivateGrinding == true do
                    pcall(function()
                        if getgenv().FoundAnything == true and not game:GetService("Players").LocalPlayer.PlayerGui.InCombat.Enabled then
                            if game:GetService("Players").LocalPlayer.Data.Achievements["The Intelligent"] and game:GetService("Players").LocalPlayer.Data.AppliedACM.Value ~= "The Intelligent" then
                                local args = {[1] = "The Intelligent"};
                                game:GetService("ReplicatedStorage"):WaitForChild("GlobalUsedRemotes"):WaitForChild("ApplyAchievement"):FireServer(unpack(args));
                            end
                        elseif getgenv().FoundAnything == false and not game:GetService("Players").LocalPlayer.PlayerGui.InCombat.Enabled then
                            if game:GetService("Players").LocalPlayer.Data.Achievements["The Intelligent"] and game:GetService("Players").LocalPlayer.Data.AppliedACM.Value ~= "The Intelligent" then
                                local args = {[1] = "The Intelligent"};
                                game:GetService("ReplicatedStorage"):WaitForChild("GlobalUsedRemotes"):WaitForChild("ApplyAchievement"):FireServer(unpack(args));
                            end
                        elseif getgenv().FoundAnything == false and game:GetService("Players").LocalPlayer.PlayerGui.InCombat.Enabled then
                            game.Players.LocalPlayer.Character.Humanoid.Health = 0
                        end            
                    end)
                    task.wait(0.35);
                end
            end)
        
            task.spawn(function()
                while getgenv().AutoPrivateGrinding == true do
                        pcall(function()
                            if getgenv().FoundAnything == false and not game:GetService("Players").LocalPlayer.PlayerGui.InCombat.Enabled then
                                if game:GetService("Players").LocalPlayer.Data.Achievements["The Intelligent"] and game:GetService("Players").LocalPlayer.Data.AppliedACM.Value == "The Intelligent" then
                                    local RespawnNeeded = false
        
                                    for _,k in pairs(game.Workspace.Item:GetChildren()) do
                                        if ((k.Name == "Box" and k:FindFirstChild("Outline")) and k.Position ~= Vector3.new(1804.5390625,-298.173583984375,-309.4649658203125)) or k.Name == "Barrel" or k.Name == "Chest" and game:GetService("Players").LocalPlayer.Backpack:FindFirstChild("Chest Key") then
