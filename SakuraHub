                            end
                        else
                            task.wait(2.5);
                        end
                    end)
                    task.wait(0.015);
                end
            end)
        
            task.spawn(function()
                while getgenv().AutoPrivateGrinding == true do
                    pcall(function()
                        if getgenv().FoundAnything == true and not game:GetService("Players").LocalPlayer.PlayerGui.InCombat.Enabled then
                            if game:GetService("Players").LocalPlayer.Data.Achievements["The Intelligent"] and game:GetService("Players").LocalPlayer.Data.AppliedACM.Value ~= "The Intelligent" then
                                local args = {[1] = "The Intelligent"};
                                game:GetService("ReplicatedStorage"):WaitForChild("GlobalUsedRemotes"):WaitForChild("ApplyAchievement"):FireServer(unpack(args));
                            end
                        elseif getgenv().FoundAnything == false and not game:GetService("Players").LocalPlayer.PlayerGui.InCombat.Enabled then
                            if game:GetService("Players").LocalPlayer.Data.Achievements["The Intelligent"] and game:GetService("Players").LocalPlayer.Data.AppliedACM.Value ~= "The Intelligent" then
                                local args = {[1] = "The Intelligent"};
                                game:GetService("ReplicatedStorage"):WaitForChild("GlobalUsedRemotes"):WaitForChild("ApplyAchievement"):FireServer(unpack(args));
