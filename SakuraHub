-- // Toggle UI Visibility \\ --
task.spawn(function()
    if not getgenv().ToggleVisibility == true then
        game:GetService("UserInputService").InputBegan:Connect(function(v,x)
            if not x and v.KeyCode == Enum.KeyCode[getgenv().ToggleUI] then
                pcall(function()
                    game:GetService("CoreGui").DrRay.TopBar.Visible = not game:GetService("CoreGui").DrRay.TopBar.Visible
                    game:GetService("CoreGui").DrRay.MainBar.Visible = not game:GetService("CoreGui").DrRay.MainBar.Visible
                end)
            end
        end)
        getgenv().ToggleVisibility = true
    end
end)
