                                    end
                                end
                            end
                        end)
                    task.wait(0.35);
                end
            end)
    
            task.spawn(function()
                while getgenv().AutoPrivateGrinding == true do
                    pcall(function()
                        if getgenv().FoundAnything == false and not game:GetService("Players").LocalPlayer.PlayerGui.InCombat.Enabled then
                            local Lplayer = game:GetService("Players").LocalPlayer

                            local A1 = game:GetService("Workspace"):FindFirstChild("Item");
                            local A2 = game:GetService("Workspace"):FindFirstChild("Item2");
                            
                            local Parts = {}
                            
                            local function Checks(Item)
                                if Item then
                                    for _,k in ipairs(Item:GetChildren()) do
                                        if k:IsA("BasePart") and k.Name ~= "SoulSpawn" and k.Name ~= "PerceptionMask" and k.Position ~= Vector3.new(1804.5390625,-298.173583984375,-309.4649658203125) then
                                            if (((k.Name:sub(1,2) == "Bo" or k.Name:sub(1,3) == "Sec") and k:FindFirstChild("Outline"))) or (((k.Name:sub(1,2) ~= "Bo" and k.Name:sub(1,3) ~= "Sec") and not k:FindFirstChild("Outline"))) then
                                                if not ((Lplayer.Data.Mastery.Value == 0) and k.Name == "SukunaFinger") and not ((Lplayer.Data.Mastery.Value == 0) and k.Name == "Hogyoku Fragment") then
                                                    if (Lplayer.Backpack:FindFirstChild("Chest Key") and game.Workspace.Item:FindFirstChild("Chest") and k.Name == "Chest") or (not Lplayer.Backpack:FindFirstChild("Chest Key") and game.Workspace.Item:FindFirstChild("Chest") and k.Name ~= "Chest") then
                                                        table.insert(Parts,k);
                                                    elseif not game.Workspace.Item:FindFirstChild("Chest") then
                                                        table.insert(Parts,k);
                                                    end
                                                end
                                            end
                                        end
                                    end
                                end
                            end
                            
                            Checks(A1);Checks(A2);
                            
                            if #Parts > 0 then
                                local Selected = Parts[math.ceil((1 - math.sqrt(math.random())) * #Parts)];

                                if Selected then
                                    Lplayer.Character:SetPrimaryPartCFrame(CFrame.new(Selected.Position - Vector3.new(0,-2.5,0)));task.wait(0.15);
                                    for _,Items in ipairs({A1,A2}) do
