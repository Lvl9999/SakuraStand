ShopTab.newLabel("Quick Seller  🏬");
ShopTab.newToggle("Auto Sell Arrow","",false,function(Value)
    getgenv().AutoSellingArrow = Value

    task.spawn(function()
        while getgenv().AutoSellingArrow == true do
            pcall(function()
                for i,v in ipairs(game:GetService("Players").LocalPlayer:WaitForChild("Backpack"):GetChildren()) do
                    if table.find({"Arrow"},v.Name) then
                        game:GetService("ReplicatedStorage"):WaitForChild("GlobalUsedRemotes"):WaitForChild("SellItem"):FireServer(v.Name);
                    end
                end
            end)
            task.wait(3.5);
        end
    end)
end)

ShopTab.newToggle("Auto Sell Rokakaka","",false,function(Value)
    getgenv().AutoSellingRokaka = Value

    task.spawn(function()
        while getgenv().AutoSellingRokaka == true do
            pcall(function()
                for i,v in ipairs(game:GetService("Players").LocalPlayer:WaitForChild("Backpack"):GetChildren()) do
                    if table.find({"Rokakaka"},v.Name) then
                        game:GetService("ReplicatedStorage"):WaitForChild("GlobalUsedRemotes"):WaitForChild("SellItem"):FireServer(v.Name);
                    end
                end
            end)
            task.wait(3.5);
        end
    end)
end)

