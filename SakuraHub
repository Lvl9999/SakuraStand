local FarmingTab = DrRayLibrary.newTab("Farming","http://www.roblox.com/asset/?id=12334656615");

FarmingTab.newLabel("Equip Best Farming Spec   🛠️");
FarmingTab.newDropdown("","",{"Shinra Kusakabe"},function(Value)
    pcall(function()
        if game:GetService("Players").LocalPlayer.PlayerGui.InCombat.Enabled then
            BoredLibrary.prompt("Sakura Hub   🌸","You are Combat Marked...",0.75);task.wait(0.15);BoredLibrary.prompt("Sakura Hub   🌸","Reseting Character...",0.75);
            local args = {[1] = "kill"};
            game:GetService("ReplicatedStorage"):WaitForChild("GlobalUsedRemotes"):WaitForChild("SukunaDialogue"):FireServer(unpack(args));
        else
            if game:GetService("Players").LocalPlayer.Data.StandName.Value ~= Value then
                task.spawn(function()BoredLibrary.prompt("Sakura Hub   🌸","Changing Spec...",0.5);end);
                for i = 1,100 do
                    if game:GetService("Players").LocalPlayer.PlayerGui.StandStorage.Outer.Inner.Inner["Slot"..i].Text.Text == Value then
                        local args = {"Slot"..i};
                        if i <= 6 then
                            game:GetService("ReplicatedStorage").StorageRemote["Slot"..i]:FireServer();
                        else
                            game:GetService("ReplicatedStorage").StorageRemote.UseStorageExtra:FireServer(unpack(args));
                        end
                    end
                end
            else
                task.spawn(function()BoredLibrary.prompt("Sakura Hub   🌸","You are Already "..Value,0.5);end);
            end
        end
    end)
end)

FarmingTab.newLabel("Equip Your Title   📌");
local Table = {}
local Intelligent = false;

for i,v in ipairs(game:GetService("Players").LocalPlayer.Data.Achievements:GetChildren()) do
    if v.Name == "The Intelligent" then
        Intelligent = true
    else
        table.insert(Table,v.Name);
    end
end

if Intelligent then
    table.insert(Table,1,"The Intelligent");
end

FarmingTab.newDropdown("","",Table,function(Value)
    pcall(function()
        getgenv().SelectedTitle = Value;
    end)
end)

FarmingTab.newToggle("Auto Equip Selected Title","",false,function(Value)
    getgenv().AutoSelectTitle = Value

    task.spawn(function()
        while getgenv().AutoSelectTitle == true do
            pcall(function()
                if not game:GetService("Players").LocalPlayer.PlayerGui.InCombat.Enabled and game:GetService("Players").LocalPlayer.Data.AppliedACM.Value ~= getgenv().SelectedTitle then
                    local args = {getgenv().SelectedTitle};
                    game:GetService("ReplicatedStorage"):WaitForChild("GlobalUsedRemotes"):WaitForChild("ApplyAchievement"):FireServer(unpack(args));
                end
            end)
            task.wait(1.5);
        end
    end)
end)

